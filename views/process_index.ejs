<% include partials/header %>


	<div class = "container">
		<div class="jumbotron">
	  		<h1>Process Search</h1>
	  		<div class="row">
	  			<div class="input-group input-group-lg col-lg-6">
					  <span class="input-group-addon" id="sizing-addon1">Key <i class="fas fa-key"></i></span>
					  <input type="number" class="form-control" placeholder="Process ID" aria-describedby="sizing-addon1">
				</div>
				<span><button class="btn btn-default btn-lg col-lg-6">Search</button></span>
	  		</div>
	  		<hr>
			<table class="table">
			  <thead>
			    <tr>

			      <th scope="col">Process ID</th>
			      <th scope="col">Name</th>
			      <th scope="col">Cost (RM)</th>
			      <th scope="col">Time</th>
			    </tr>
			  </thead>			
			  <tbody>
				<% processes.forEach(function(process){ %>					  
					<tr>
					  <th scope="row"><%= process.processId %></th>
					  <td><%= process.processName %></td>
					  <td><%= process.processCost %></td>
					  <td><%= process.processTime %></td>
					</tr>
				<% }); %>
			  </tbody>
			</table>

		</div>
			
	</div>



<% include partials/footer %>